<GameData>
	<Table name="Unit_FreeUnitAfterSurvivingMeleeCombat">
		<Column name="UnitType" type="text" reference="Units(Type)"/>
		<Column name="UnitClassType" type="text" reference="UnitClasses(Type)"/>
		<Column name="NumberFree" type="integer" default="0"/>
	</Table>
	<Table name="Unit_FreeUnitWhenTradeRoutePlundered">
		<Column name="UnitType" type="text" reference="Units(Type)"/>
		<Column name="UnitClassType" type="text" reference="UnitClasses(Type)"/>
		<Column name="NumberFree" type="integer" default="0"/>
	</Table>
	<Table name="Unit_FreePromotionsUntilProjectCompleted">
		<Column name="UnitType" type="text" reference="Units(Type)"/>
		<Column name="PromotionType" type="text" reference="UnitPromotions(type)"/>
		<Column name="ProjectType" type="text" reference="Projects(Type)"/>
	</Table>
	<Table name="Unit_OnePowerWielding">
		<Column name="UnitType" type="text" reference="Units(Type)"/>
		<Column name="OnePowerType" type="text" reference="OnePowers(Type)"/>
	</Table>
	<Table name="Unit_BondedFreePromotions">
		<Column name="UnitType" type="text" reference="Units(Type)"/>
		<Column name="PromotionType" type="text" reference="UnitPromotions(Type)"/>
	</Table>
	<Table name="Unit_BondBreakPromotions">
		<Column name="UnitType" type="text" reference="Units(Type)" />
		<Column name="PromotionType" type="text" reference="UnitPromotions(Type)" />
	</Table>
</GameData>